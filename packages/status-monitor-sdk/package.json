{
  "name": "@status-monitor/sdk",
  "version": "1.0.0",
  "description": "SDK for sending heartbeats to Status Monitor from internal services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "status-monitor",
    "heartbeat",
    "monitoring",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@workspace/typescript-config": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.5.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=22"
  }
}